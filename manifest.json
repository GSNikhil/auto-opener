{
    "manifest_version": 2,
    "name": "AutoOpener",
    "version": "1.0",
    "description": "Opens a set of websites every day at 9 PM.",
    "permissions": [
      "tabs",
      "alarms",
      "http://*/*",
      "https://*/*"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "icons": {
      "48": "icon.png"
    },
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    }
  }
  